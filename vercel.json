{
  "version": 2,
  "name": "portal-dr-marcio",
  "buildCommand": "npm run inject-env",
  "outputDirectory": ".",
  "installCommand": "",
  "framework": null,
  "public": true,
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/auth/login.html"
    },
    {
      "source": "/cadastro",
      "destination": "/auth/cadastro.html"
    },
    {
      "source": "/recuperar-senha",
      "destination": "/auth/redefinir-senha.html"
    },
    {
      "source": "/dashboard",
      "destination": "/home.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/dashboard-admin.html"
    },
    {
      "source": "/agendamentos",
      "destination": "/modules/agendamentos.html"
    },
    {
      "source": "/pacientes",
      "destination": "/modules/pacientes.html"
    },
    {
      "source": "/prontuarios",
      "destination": "/modules/prontuarios.html"
    },
    {
      "source": "/caderno-digital",
      "destination": "/modules/caderno-digital.html"
    },
    {
      "source": "/configuracoes",
      "destination": "/pages/configuracoes-sistema.html"
    },
    {
      "source": "/pre-cadastro",
      "destination": "/pages/pre-cadastro.html"
    },
    {
      "source": "/orcamento",
      "destination": "/pages/orcamento.html"
    },
    {
      "source": "/agendamento-detalhes",
      "destination": "/pages/agendamento-detalhes.html"
    },
    {
      "source": "/agendar",
      "destination": "/pages/agendar.html"
    },
    {
      "source": "/404",
      "destination": "/pages/404.html"
    },
    {
      "source": "/500",
      "destination": "/pages/500.html"
    },
    {
      "source": "/offline",
      "destination": "/pages/offline.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdnjs.cloudflare.com https://cdn.jsdelivr.net; object-src 'none'; base-uri 'self';"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}
