{
    "version": "1.0.0-secure",
    "environment": "production",
    "security": {
        "sessionTimeout": 1800000,
        "maxLoginAttempts": 3,
        "passwordMinLength": 8,
        "encryptionEnabled": true,
        "auditEnabled": true,
        "backupEnabled": true
    },
    "features": {
        "debugMode": false,
        "consoleLogging": false,
        "developmentMode": false
    },
    "compliance": {
        "lgpd": true,
        "cfm": true,
        "dataRetention": "20years",
        "auditRetention": "5years"
    }
}
